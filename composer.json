{
	"name": "codekandis/grabdatapes",
	"description": "Fetches all downloadable content from `http://artmeetsscience.co.uk/tapes`",
	"keywords": [
		"php",
		"grabdatapes"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/grabdatapes/issues",
		"docs": "https://github.com/codekandis/grabdatapes/blob/master/README.md",
		"source": "https://github.com/codekandis/grabdatapes/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/tiphy": "^0",
		"codekandis/sentry-client": "1",
		"codekandis/tiphy-sentry-client-integration": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\GrabDaTapes\\": [
				"config/",
				"public/",
				"src/",
				"templates/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\GrabDaTapes\\Tests\\": [
				"tests/"
			]
		}
	}
}
